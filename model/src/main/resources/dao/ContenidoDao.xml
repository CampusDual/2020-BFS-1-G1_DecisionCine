<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup
        xmlns="http://www.ontimize.com/schema/jdbc"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
        catalog="" schema="${mainschema}" table="contenido"
        datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
    <DeleteKeys>
        <Column>contenido_id</Column>
    </DeleteKeys>
    <UpdateKeys>
        <Column>contenido_id</Column>
    </UpdateKeys>
    <GeneratedKey>contenido_id</GeneratedKey>
    <Queries>
        <Query id="pelisMasPuntuadas">
            <AmbiguousColumns>
                <AmbiguousColumn name="contenido_id" prefix="contenido"/>
            </AmbiguousColumns>
            <Sentence><![CDATA[select * from contenido
            order by contenido_puntuacion desc limit 5]]>
            </Sentence>
        </Query>
    </Queries>
</JdbcEntitySetup>